{
  "schemaVersion": 1,
  "id": "nullmovement",
  "version": "${version}",
  "name": "Null Movement",
  "description": "Allows for smoother movement in Minecraft by preventing movement blocking, inspired by the Source script of the same name",
  "authors": [
    "Fruitloopins"
  ],
  "contact": {
    "homepage": "https://fwloopins.com",
    "sources": "https://github.com/Fruitloopins/NullMovement",
    "issues": "https://github.com/Fruitloopins/NullMovement/issues"
  },
  "license": "MIT",
  "icon": "assets/nullmovement/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.fwloopins.nullmovement.NullMovement"
    ]
  },
  "mixins": [
    "nullmovement.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Discord": "https://discord.gg/ey6ZvnwAJp"
      }
    }
  }
}
